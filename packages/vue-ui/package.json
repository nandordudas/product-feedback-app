{
  "name": "@product-feedback-app/vue-ui",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/vue-ui.umd.cjs",
      "import": "./dist/vue-ui.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/vue-ui.umd.cjs",
  "module": "./dist/vue-ui.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --skipLibCheck",
    "coverage": "vitest --coverage",
    "dev": "vite",
    "lint": "eslint --cache .",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@vue-macros/volar": "^0.9.5",
    "unocss": "^0.51.5",
    "unplugin-vue-macros": "^2.0.0"
  }
}
