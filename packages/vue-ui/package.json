{
  "name": "@product-feedback-app/vue-ui",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/vue-ui.umd.cjs",
      "import": "./dist/vue-ui.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/vue-ui.umd.cjs",
  "module": "./dist/vue-ui.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --skipLibCheck",
    "coverage": "vitest --coverage",
    "dev": "vite",
    "lint": "eslint --cache .",
    "preview": "vite preview",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "^0.4.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/browser": "^0.30.1",
    "@vitest/coverage-c8": "^0.30.1",
    "@vitest/ui": "^0.30.1",
    "@vue-macros/volar": "^0.9.5",
    "@vue/test-utils": "^2.3.2",
    "happy-dom": "^9.8.4",
    "typescript": "^5.0.4",
    "unocss": "^0.51.5",
    "unplugin-vue-macros": "^2.0.0",
    "vite": "^4.3.1",
    "vite-plugin-inspect": "^0.7.23",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.4.1"
  }
}
